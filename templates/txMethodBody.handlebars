{{#if simple}}
    tx.simple({{#simple}}
      `{{{name}}}`{{#unless @last}},{{/unless}}{{/simple}}
    );{{/if}}{{#named}}    tx.named(`{{{name}}}`{{#if mapping}}, {{{mapping}}}{{/if}});
{{/named}}{{#positional}}    tx.positional(`{{{name}}}`{{#if mapping}}, {{{mapping}}}{{/if}});
{{/positional}}{{#if nested}}
    tx.nested({{#nested}}
      {{@root.metaClassName}}._tx{{className}}(`{{{property}}}`),{{/nested}}
    );{{/if}}
    return tx;
